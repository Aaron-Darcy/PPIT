<style>
    table{
        background-color: white;
    }
    tb,
    td {
        border: 1px solid black;
        
    }
   body{
       background-color: teal;
   }
   H1{
       color: white;
   }
</style>
<body>
<img src="data:image/jpeg;base64," width=100, height=100>
<form action="/newDays">
    <button>set previous days to 'none'</button>
    </form>
    <form action="/SetCurrentDays" method="GET">
        <button>set days worked out</button>
    </form>

<table>
  
    <H1>Days worked out</H1>
    <tr>
    
        <th>Monday</th>
        <th>Tuesday</th>
        <th>Wednesday</th>
        <th>Thursday</th>
        <th>Friday</th>
        <th>Saturday</th>
        <th>Sunday</th>
        
        <H3><th>current week</th></H3>
    </tr>
   
  
    <% day.forEach((day)=> { %>
    <tr>
        <td><%= day.Monday%> </td>
        <td><%= day.Tuesday%> </td>
        <td><%= day.Wednesday%> </td>
        <td><%= day.Thursday%> </td>
        <td><%= day.Friday%> </td>
        <td><%= day.Saturday%> </td>
        <td><%= day.Sunday%> </td>
    </tr>
    <% }) %>
   
   
    <th>Monday</th>
    <th>Tuesday</th>
    <th>Wednesday</th>
    <th>Thursday</th>
    <th>Friday</th>
    <th>Saturday</th>
    <th>Sunday</th>
    <H3><th>previous week</th></H3>
    <% day.forEach((day)=> { %>
    <tr>
        <td><%= day.monday%> </td>
        <td><%= day.tuesday%> </td>
        <td><%= day.wednesday%> </td>
        <td><%= day.thursday%> </td>
        <td><%= day.friday%> </td>
        <td><%= day.saturday%> </td>
        <td><%= day.sunday%> </td>

      
    </tr>  
     <% }) %>
    
     <tr>
        <th>userID:</th>
    </tr>

    <% day.forEach((day)=> { %>
        <tr>
           <td> <%=day.IDno%> </td>

           <form action="/ResetDay/<%= day.IDno %>">
            <button>reset days</button>
            </form>

            
        </tr>
    <% }) %>
</table>
<form action="/UserMenu">
    <button>return to menu</button>
    </form>
</body>